<!DOCTYPE html>
<html lang="fr">
	<head>
		<title>
			{% block title %}DigitLight - Connexion
			{% endblock %}
		</title>
		<meta name="description" content="Le gestionnaire de stock pour le projet Icademie">
		<meta name="author" content="DigitLight">
		<meta name="viewport" content="width=device-width, initial-scale=1">
		<meta charset="utf-8">
		<link rel="icon" href="/build/images/favicon.png">

		{% block stylesheets %}
			{# {{ encore_entry_link_tags('css/global') }} #}
		{% endblock %}

		{% block javascripts %}
			{{ encore_entry_script_tags('js/app') }}

			<!-- Google tag (gtag.js) -->
			<script async src="https://www.googletagmanager.com/gtag/js?id=G-V7SZXWSJP4"></script>
			<script>
		    window.dataLayer = window.dataLayer || [];
		    function gtag() {
		    dataLayer.push(arguments);
		    }
		    gtag('js', new Date());
		    gtag('config', 'G-V7SZXWSJP4');
			</script>

			<!-- Google API -->
            <script src="https://apis.google.com/js/api.js" type="text/javascript"></script>

		{% endblock %}
	</head>
	<body>
		{% block body %}
			<div {{ react_component('accueil') }}></div>
		{% endblock %}
	</body>
</html>
